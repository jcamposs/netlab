      <div class="navbar">
        <div class="navbar-inner">
          <a href="#" class="brand"><%= "#{@user.first_name}'s tasks" %></a>
          <span class="icon pull-right">
            <i class="icon-globe"></i>
          </span>
        </div>
      </div>

      <table class="table table-hover table-list">
        <% @user.assigned_workspace_tasks.where("state < 5").each do |wtask| %>
          <tbody>
            <tr>
              <td class="list-image"><i class="icon-envelope"></i></td>
              <td><%= "#{wtask.workspace.name}" %></td>
              <td><%= "#{wtask.subject}" %></td>
              <td><%= '<span class="minor">'.html_safe + ("#{wtask.created_at}").to_time.strftime("%B %d %Y %X") + '</span>'.html_safe %>
              </td>
              <td>
                <% if not wtask.auto_task.nil? %>
                  <%= link_to '<i class="icon-ok icon-white"></i> Ok'.html_safe, do_auto_task_workspace_task_path(wtask), method: :put, :class => 'btn btn-mini btn-success' %>
                <% else %>
                  <%= link_to '<i class="icon-ok icon-white"></i> Show'.html_safe, wtask, :class => 'btn btn-mini btn-success' %>
                <% end %>
                <%= link_to '<i class="icon-remove icon-white"></i> Reject'.html_safe, wtask, method: :delete, data: { confirm: 'Are you sure?' }, :class => 'btn btn-mini btn-danger' %>
              </td>
            </tr>
          </tbody>
        <% end %>
      </table>
